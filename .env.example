NODE_ENV=
APP_DEBUG=true
PORT=3001
DATABASE_URL="postgresql://postgres:password@localhost:5432/career_skill_aligner?schema=public"
TEST_DATABASE_URL="postgresql://testuser:testpassword123@localhost:5434/testdb?schema=public"

# API Keys (optional - only need the key for the provider you use)
OPENAI_API_KEY=
OPENROUTER_API_KEY=

# Embedding configuration
SEMANTICS_API_BASE_URL='http://localhost:8000/api/v1/semantics'
EMBEDDING_MODEL='e5-base'  # Options: 'e5-base', 'text-embedding-3-small'
# EMBEDDING_PROVIDER='local'  # Deprecated: Use EMBEDDING_MODEL instead

# OpenRouter configuration
OPENROUTER_BASE_URL='https://openrouter.ai/api/v1'

# LLM configuration
DEFAULT_LLM_PROVIDER='openrouter'  # Options: 'openrouter', 'openai'

# Mock service toggles
USE_MOCK_QUESTION_CLASSIFIER_SERVICE=false
USE_MOCK_QUERY_PROFILE_BUILDER_SERVICE=false
USE_MOCK_SKILL_EXPANDER_SERVICE=false

# LLM Models (format: "provider/model-name" or just "model-name" to use default provider)
QUESTION_CLASSIFIER_LLM_MODEL='openrouter/orca-mini-3b-v2'
QUERY_PROFILE_BUILDER_LLM_MODEL='openrouter/orca-mini-3b-v2'
SKILL_EXPANDER_LLM_MODEL='openrouter/orca-mini-3b-v2'
FILTER_LO_LLM_MODEL='openrouter/orca-mini-3b-v2'
ANSWER_SYNTHESIS_LLM_MODEL='openrouter/orca-mini-3b-v2'
COURSE_RELEVANCE_FILTER_LLM_MODEL='openrouter/orca-mini-3b-v2'

# Cache toggles
USE_QUESTION_CLASSIFIER_CACHE=true
USE_SKILL_EXPANDER_CACHE=true
